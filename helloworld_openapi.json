{
  "openapi": "3.0.0",
  "info": {
    "title": "Sample API",
    "version": "0.1.9"
  },
  "servers": [
    {
      "url": "http://localhost:9090"
    }
  ],
  "paths": {
    "/HelloWorld": {
      "get": {
        "summary": "Returns a Hello message.",
        "description": "Optional extended description in CommonMark or HTML.",
        "responses": {
          "200": {
            "description": "A JSON of Hello message",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string",
                  "items": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
